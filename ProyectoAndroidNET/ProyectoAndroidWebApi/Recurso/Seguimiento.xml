    </resultMap>
  </resultMaps>
  <parameterMaps>
    <parameterMap id="paramSeguimientoSEL">
      <parameter property="ART_COD" direction="input"/>
    </parameterMap>
  </parameterMaps>
  <statements>
    <procedure id="uspSeguimientoSEL" resultMap="resultMapSeguimiento" parameterMap="paramSeguimientoSEL">
        USP_SEGUIMIENTO_SEL
    </procedure>
  </statements>  
</sqlMap>